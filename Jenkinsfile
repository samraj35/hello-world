pipeline{
agent any 
Stages{
      stage("checkout"){
            steps{
                 echo ' Checking out the code'
            }
            }
      stage("Build"){
            steps{
                 echo ' Building the code'
            }
            }
      stage("Test"){
            steps{
                 echo ' Testing out the code'
            }
            }
      stage("Deploy"){
            steps{
                 echo ' Deploying out the code'
            }
            }
}
}
